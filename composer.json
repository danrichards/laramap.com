{
    "name": "laramap/laramap.com",
    "description": "The Laramap.com source.",
    "keywords": ["laramap", "laravel", "community"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel-notification-channels/twitter": "^1.0",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.3",
        "laravel/passport": "^6.0",
        "laravel/scout": "^5.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.1",
        "rennokki/befriended": "^1.1",
        "spatie/laravel-cors": "^1.2",
        "spatie/laravel-csp": "^1.3",
        "spatie/laravel-feed": "^2.1",
        "spatie/laravel-newsletter": "^4.2",
        "spatie/laravel-permission": "^2.12",
        "spatie/laravel-query-builder": "^1.10",
        "spatie/laravel-referer": "^1.3",
        "vinkla/hashids": "^5.0",
        "get-stream/stream-laravel": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
